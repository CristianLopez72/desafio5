    <%- include('../parciales/header'); %>
    <%- include('../parciales/linkformulario'); %>
    <table style="width:50%">
      <tr>
        <th id="id">ID</th>
        <th id="nombreHeader">Nombre</th>
        <th id="categoriaHeader">Categoria</th>
        <th id="precioHeader"> Precio</th>
        <th id="fotoHeader"> Foto</th>    
      </tr>
      <% productos.forEach(anItem=> { %>
        <tr>
          <td>
            <%= anItem.id %>
          </td>
          <td>
            <%= anItem.nombre %>
          </td>
          <td>
            <%= anItem.categoria %>
          </td>
          <td>
            <%= anItem.precio %>
          </td>
          <td>
            <img src="<%= anItem.foto %>" width="60" height="60" >
          </td>
        </tr>
        <% }) %>
    </table>
